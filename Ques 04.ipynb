    "def nonRepeat():\n",
    "    string = input('give the string:')\n",
    "    for i in string:\n",
    "        if (string.find(i, (string.find(i)+1))) == -1:\n",
    "            print(\"First non-repeating character is\", i)\n",
    "            break\n",
    "    return\n",
    "\n",
    "nonRepeat()"
   ]
